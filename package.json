{
  "name": "wdio-mocha",
  "version": "1.0.0",
  "description": "WebdriverIO boilerplate using WebdriverIO, Mocha, and TypeScript",
  "scripts": {
    "test": "npm run build && wdio ./wdio.conf.js",
    "build": "tsc -p .",
    "prepare": "husky install",
    "lint": "eslint --ext .js,.ts .",
    "format": "prettier --write --ignore-unknown .",
    "test-stage-bs": "wdio resources/config/wdio.stage.browserstack.conf.ts",
    "test-stage-onprem": "wdio resources/config/wdio.stage.on-prem.conf.ts",
    "test-prod-bs": "wdio resources/config/wdio.prod.browserstack.conf.ts"
    "format": "prettier --write --ignore-unknown ."
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@wdio/cli": "^7.6.1",
    "@wdio/local-runner": "^7.6.1",
    "@wdio/mocha-framework": "^7.6.0",
    "browserstack-local": "^1.4.8",
    "chai": "^4.3.4",
    "minimist": "^1.2.5",
    "webdriverio": "^7.6.1"
  },
  "devDependencies": {
    "@types/chai": "^4.2.18",
    "@types/minimist": "^1.2.1",
    "@types/mocha": "^8.2.2",
    "@types/node": "^14.17.1",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.0",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.2"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --ext .js,.ts"
    ],
    "*.{js,jsx,ts,tsx,html,css,json,md,yaml,yml,graphql}": [
      "prettier --write --ignore-unknown"
    ]
  }
}

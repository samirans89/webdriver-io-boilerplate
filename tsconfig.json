{
  "compilerOptions": {
    "outDir": "build/",
    "sourceMap": false,
    "target": "es2020",
    "module": "commonjs",
    "removeComments": true,
    "noImplicitAny": true,
    "alwaysStrict": true,
    "strictPropertyInitialization": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "strictNullChecks": true,
    "skipLibCheck": true,
    "types": ["webdriverio/sync", "chai", "mocha"]
  }
}
